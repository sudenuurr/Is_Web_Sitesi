    <link rel="stylesheet" href="~/jobportal/assets/css/fontawesome-all.min.css">
    <link rel="stylesheet" href="~/jobportal/assets/css/themify-icons.css">
    <link rel="stylesheet" href="~/jobportal/assets/css/slick.css">
    <link rel="stylesheet" href="~/jobportal/assets/css/nice-select.css">
    <link rel="stylesheet" href="~/jobportal/assets/css/style.css">
</head>

<body>
    <!-- Preloader Start -->
    <div id="preloader-active">
        <div class="preloader d-flex align-items-center justify-content-center">
            <div class="preloader-inner position-relative">
                <div class="preloader-circle"></div>
                <div class="preloader-img pere-text">
                    <img src="~/jobportal/assets/img/logo/job-finding-5150521-4309625.webp" alt="">
                </div>
            </div>
        </div>
    </div>
    <!-- Preloader Start -->

    @await Html.PartialAsync("PartialHeader")

    @RenderBody()

    @await Html.PartialAsync("PartialFooter")

    <!-- JS here -->
    <!-- All JS Custom Plugins Link Here here -->
    <script src="~/jobportal/assets/js/vendor/modernizr-3.5.0.min.js"></script>
    <!-- Jquery, Popper, Bootstrap -->
    <script src="~/jobportal/assets/js/vendor/jquery-1.12.4.min.js"></script>
    <script src="~/jobportal/assets/js/popper.min.js"></script>
    <script src="~/jobportal/assets/js/bootstrap.min.js"></script>
    <!-- Jquery Mobile Menu -->
    <script src="~/jobportal/assets/js/jquery.slicknav.min.js"></script>

    <!-- Jquery Slick , Owl-Carousel Plugins -->
    <script src="~/jobportal/assets/js/owl.carousel.min.js"></script>
    <script src="~/jobportal/assets/js/slick.min.js"></script>
    <script src="~/jobportal/assets/js/price_rangs.js"></script>

    <!-- One Page, Animated-HeadLin -->
    <script src="~/jobportal/assets/js/wow.min.js"></script>
    <script src="~/jobportal/assets/js/animated.headline.js"></script>
    <script src="~/jobportal/assets/js/jquery.magnific-popup.js"></script>

    <!-- Scrollup, nice-select, sticky -->
    <script src="~/jobportal/assets/js/jquery.scrollUp.min.js"></script>
    <script src="~/jobportal/assets/js/jquery.nice-select.min.js"></script>
    <script src="~/jobportal/assets/js/jquery.sticky.js"></script>

    <!-- contact js -->
    <script src="~/jobportal/assets/js/contact.js"></script>
    <script src="~/jobportal/assets/js/jquery.form.js"></script>
    <script src="~/jobportal/assets/js/jquery.validate.min.js"></script>
    <script src="~/jobportal/assets/js/mail-script.js"></script>
    <script src="~/jobportal/assets/js/jquery.ajaxchimp.min.js"></script>

    <!-- Jquery Plugins, main Jquery -->
    <script src="~/jobportal/assets/js/plugins.js"></script>
    <script src="~/jobportal/assets/js/main.js"></script>

