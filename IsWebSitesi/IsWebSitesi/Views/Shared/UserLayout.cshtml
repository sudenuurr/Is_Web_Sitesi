
@{
    Layout = null;
}

<!doctype html>
<html class="no-js" lang="zxx">
<head>
    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <title>İş Web Sitesi </title>
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="manifest" href="site.webmanifest">
    <link rel="shortcut icon" type="image/x-icon" href="~/jobportal/assets/img/favicon.ico">

    <!-- CSS here -->
    <link rel="stylesheet" href="~/jobportal/assets/css/bootstrap.min.css">
    <link rel="stylesheet" href="~/jobportal/assets/css/owl.carousel.min.css">
    <link rel="stylesheet" href="~/jobportal/assets/css/flaticon.css">
    <link rel="stylesheet" href="~/jobportal/assets/css/price_rangs.css">
    <link rel="stylesheet" href="~/jobportal/assets/css/slicknav.css">
